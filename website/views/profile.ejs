<!-- web/views/profile.ejs -->
<!DOCTYPE html>
<html>
<head>
    <title>User Profile</title>
</head>
<body>
    <h1>User Profile</h1>

    <h2>Discord ID: <%= user.discordId %></h2>
    <h2>In-Game ID: <%= user.inGameId %></h2>
    <% if (user.profilePictureUrl) { %>
        <img src="<%= user.profilePictureUrl %>" alt="Profile Picture">
    <% } %>
    <!-- Display other user profile fields as needed -->
</body>
</html>
